{
  "name": "read-action",
  "version": "1.2.11",
  "description": "This GitHub action tracks the books that you've read by updating the `data/read.json` file in your repository.",
  "keywords": [
    "library",
    "book"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "@actions/core": "^1.6",
    "@actions/github": "^5.0",
    "node-isbn": "^1.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.6.2"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "pretest": "eslint . --ext .js,.ts",
    "test": "jest --errorOnDeprecated",
    "tests": "jest --coverage --errorOnDeprecated",
    "build": "ncc build src/index.ts",
    "pretty-quick": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papposilene/action-read.git"
  },
  "author": "Philippe-Alexandre Pierre <dev@psln.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/papposilene/action-read/issues"
  },
  "homepage": "https://github.com/papposilene/action-read#readme",
  "jest": {
    "verbose": true,
    "resetMocks": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
